
public class Stock {

	private String name;
	private double price; //$.01-$9999999.99
	private double riskiness; //.01-.99
	
	//Constructor
	public Stock(String name, double price, double riskiness){
		this.name = name;
		this.price = price;
		this.riskiness = riskiness;
	}
	
	//Getters and setters
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public double getPrice() {
		return price;
	}
	
	public void setPrice(double price) {
		this.price = price;
	}
	
	public double getRiskiness() {
		return riskiness;
	}
	
	public void setRiskiness(double riskiness) {
		this.riskiness = riskiness;
	}
	
	public String toString(){
		return ("Name: " + this.name + " Current Value: $" + this.price + " Average growth/decline rate: " + this.riskiness*100 + "%");
	}
}

